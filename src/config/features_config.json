{
  "stage_name": "features",

  "input_stage": "preprocessing",
  "input": {
    "filename": ""
  },

  "output": {
    "filename": ""
  },

  "features": {
    "gt_diff_1": {
      "type": "diff",
      "column": "gt_interest",
      "lag": 1
    },
    "gt_rolling_mean_4": {
      "type": "rolling_mean",
      "column": "gt_interest",
      "window": 4
    },
    "gt_zscore_16": {
      "type": "zscore",
      "column": "gt_interest",
      "window": 16
    },
    "stock_mom_4": {
      "type": "momentum",
      "column": "SBUX",
      "window": 4
    },
    "spy_mom_4": {
      "type": "momentum",
      "column": "SPY",
      "window": 4
    },
    "xly_mom_4": {
      "type": "momentum",
      "column": "XLY",
      "window": 4
    },
    "mcd_mom_4": {
      "type": "momentum",
      "column": "MCD",
      "window": 4
    },
    "vix_diff_1": {
      "type": "diff",
      "column": "^VIX",
      "lag": 1
    },
    "vix_rm_4": {
      "type": "rolling_mean",
      "column": "^VIX",
      "window": 4
    },
    "cpi_latest_pct_change": {
      "type": "latest_pct_change",
      "column": "CPI",
      "eps": 1e-8
    },
