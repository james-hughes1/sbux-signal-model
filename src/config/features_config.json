      "type": "diff",
      "column": "price_impact",
      "lag": 1
    },
    "price_impact_rm_4": {
      "type": "rolling_mean",
      "column": "price_impact",
      "window": 4
    },
    "price_impact_z_8": {
      "type": "zscore",
      "column": "price_impact",
      "window": 8
    },

    "volatility_diff_1": {
      "type": "diff",
