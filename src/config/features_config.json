      "eps": 1e-8
    },
    "lagged_alpha": {
      "type": "lagged_alpha",
      "column": "alpha",
      "lags": [1, 2, 4, 8],
      "mas": [4, 12]
    },
    "hl_range_diff_1": {
      "type": "diff",
      "column": "hl_range",
      "lag": 1
    },
    "hl_range_rm_4": {
      "type": "rolling_mean",
      "column": "hl_range",
      "window": 4
    },
    "hl_range_z_8": {
      "type": "zscore",
      "column": "hl_range",
      "window": 8
